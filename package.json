{
  "name": "leaflet-tilelayer-swiss",
  "version": "1.0.1",
  "description": "Leaflet.TileLayer.Swiss is a Leaflet plugin for displaying national maps of Switzerland using WMTS services of swisstopo.",
  "main": "dist/Leaflet.TileLayer.Swiss.js",
  "unpkg": "dist/Leaflet.TileLayer.Swiss.umd.js",
  "scripts": {
    "build": "microbundle --name L.TileLayer.Swiss --globals leaflet=L",
    "eslint": "eslint .",
    "prepare": "npm run build",
    "start": "microbundle --watch --no-compress --name L.TileLayer.Swiss --globals leaflet=L",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rzoller/Leaflet.TileLayer.Swiss"
  },
  "files": [
    "/src",
    "/dist"
  ],
  "author": "Roman Zoller",
  "license": "MIT",
  "peerDependencies": {
    "leaflet": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-jest": "^24.8.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.15.1",
    "jest": "^24.8.0",
    "leaflet": "^1.5.1",
    "microbundle": "^0.9.0",
    "swissgrid": "^0.1.0"
  }
}

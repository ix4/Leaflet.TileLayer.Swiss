{
  "name": "leaflet-tilelayer-swiss",
  "version": "1.0.1",
  "description": "Leaflet.TileLayer.Swiss is a Leaflet plugin for displaying national maps of Switzerland using WMTS services of swisstopo.",
  "source": "src/index.js",
  "main": "dist/Leaflet.TileLayer.Swiss.js",
  "unpkg": "dist/Leaflet.TileLayer.Swiss.umd.js",
  "scripts": {
    "build": "microbundle --name L.TileLayer.Swiss --format cjs,umd --globals leaflet=L",
    "eslint": "eslint .",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rzoller/Leaflet.TileLayer.Swiss"
  },
  "files": [
    "/src",
    "/dist"
  ],
  "author": "Roman Zoller",
  "license": "MIT",
  "peerDependencies": {
    "leaflet": "^1.4.0"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "microbundle": "^0.9.0",
    "swissgrid": "^0.1.0"
  }
}
